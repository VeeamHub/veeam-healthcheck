{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Config Backup CSV Schema",
  "description": "Maps _configBackup.csv to CConfigBackupCsv C# class",
  "csvFile": "_configBackup.csv",
  "csharpClass": "VeeamHealthCheck.Functions.Reporting.CsvHandlers.CConfigBackupCsv",
  "csharpFile": "Functions/Reporting/CsvHandlers/CConfigBackupCsv.cs",
  "version": "1.0.0",
  "columns": [
    {
      "index": 0,
      "csvName": "Enabled",
      "csharpProperty": "Enabled",
      "csharpType": "string",
      "nullable": false,
      "expectedValues": ["True", "False"],
      "description": "Whether config backup is enabled"
    },
    {
      "index": 1,
      "csvName": "Repository",
      "csharpProperty": "Repository",
      "csharpType": "string",
      "nullable": true,
      "description": "Target repository name"
    },
    {
      "index": 2,
      "csvName": "ScheduleOptions",
      "csharpProperty": "ScheduleOptions",
      "csharpType": "string",
      "nullable": true,
      "description": "Schedule configuration"
    },
    {
      "index": 3,
      "csvName": "RestorePointsToKeep",
      "csharpProperty": "RestorePointsToKeep",
      "csharpType": "string",
      "nullable": true,
      "description": "Number of restore points to keep"
    },
    {
      "index": 4,
      "csvName": "EncryptionOptions",
      "csharpProperty": "EncryptionOptions",
      "csharpType": "string",
      "nullable": true,
      "description": "Encryption configuration"
    },
    {
      "index": 5,
      "csvName": "NotificationOptions",
      "csharpProperty": "NotificationOptions",
      "csharpType": "string",
      "nullable": true,
      "description": "Notification configuration"
    },
    {
      "index": 6,
      "csvName": "NextRun",
      "csharpProperty": "NextRun",
      "csharpType": "string",
      "nullable": true,
      "description": "Next scheduled run time"
    },
    {
      "index": 7,
      "csvName": "Target",
      "csharpProperty": "Target",
      "csharpType": "string",
      "nullable": true,
      "description": "Backup target"
    },
    {
      "index": 8,
      "csvName": "Type",
      "csharpProperty": "Type",
      "csharpType": "string",
      "nullable": true,
      "description": "Backup type"
    },
    {
      "index": 9,
      "csvName": "LastResult",
      "csharpProperty": "LastResult",
      "csharpType": "string",
      "nullable": true,
      "description": "Last backup result"
    },
    {
      "index": 10,
      "csvName": "LastState",
      "csharpProperty": "LastState",
      "csharpType": "string",
      "nullable": true,
      "description": "Last backup state"
    },
    {
      "index": 11,
      "csvName": "Id",
      "csharpProperty": "Id",
      "csharpType": "string",
      "nullable": false,
      "description": "Config backup GUID identifier"
    },
    {
      "index": 12,
      "csvName": "Name",
      "csharpProperty": "Name",
      "csharpType": "string",
      "nullable": false,
      "description": "Config backup name"
    },
    {
      "index": 13,
      "csvName": "Description",
      "csharpProperty": "Description",
      "csharpType": "string",
      "nullable": true,
      "description": "Config backup description"
    }
  ],
  "validationRules": {
    "requiredColumns": ["Enabled", "Id", "Name"],
    "guidColumns": ["Id"],
    "booleanColumns": ["Enabled"],
    "numericColumns": ["RestorePointsToKeep"],
    "dateTimeColumns": ["NextRun"]
  }
}
