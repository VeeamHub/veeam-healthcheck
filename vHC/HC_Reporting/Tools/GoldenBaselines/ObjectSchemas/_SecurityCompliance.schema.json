{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Security Compliance CSV Schema",
  "description": "Maps _SecurityCompliance.csv to CComplianceCsv C# class",
  "csvFile": "_SecurityCompliance.csv",
  "csharpClass": "VeeamHealthCheck.Functions.Reporting.CsvHandlers.CComplianceCsv",
  "csharpFile": "Functions/Reporting/CsvHandlers/CComplianceCsv.cs",
  "version": "1.0.0",
  "vbrVersionRequired": "12.0",
  "columns": [
    {
      "index": 0,
      "csvName": "BestPractice",
      "csharpProperty": "BestPractice",
      "csharpType": "string",
      "nullable": false,
      "description": "Security best practice name"
    },
    {
      "index": 1,
      "csvName": "Status",
      "csharpProperty": "Status",
      "csharpType": "string",
      "nullable": false,
      "expectedValues": ["Passed", "NotImplemented", "UnableToDetect", "Suppressed"],
      "description": "Compliance status"
    }
  ],
  "validationRules": {
    "requiredColumns": ["BestPractice", "Status"],
    "guidColumns": [],
    "booleanColumns": [],
    "numericColumns": []
  }
}
