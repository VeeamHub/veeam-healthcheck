{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Hyper-V Proxy CSV Schema",
  "description": "Maps _HvProxy.csv to CHvProxyCsvInfo C# class",
  "csvFile": "_HvProxy.csv",
  "csharpClass": "VeeamHealthCheck.Functions.Reporting.CsvHandlers.CHvProxyCsvInfo",
  "csharpFile": "Functions/Reporting/CsvHandlers/CHvProxyCsvInfo.cs",
  "version": "1.0.0",
  "columns": [
    {
      "index": 0,
      "csvName": "Id",
      "csharpProperty": "Id",
      "csharpType": "string",
      "nullable": false,
      "description": "Proxy GUID identifier"
    },
    {
      "index": 1,
      "csvName": "Name",
      "csharpProperty": "Name",
      "csharpType": "string",
      "nullable": false,
      "description": "Proxy display name"
    },
    {
      "index": 2,
      "csvName": "Description",
      "csharpProperty": "Description",
      "csharpType": "string",
      "nullable": true,
      "description": "Proxy description"
    },
    {
      "index": 3,
      "csvName": "HostId",
      "csharpProperty": "HostId",
      "csharpType": "string",
      "nullable": false,
      "description": "Host GUID identifier"
    },
    {
      "index": 4,
      "csvName": "Host",
      "csharpProperty": "Host",
      "csharpType": "string",
      "nullable": false,
      "description": "Host name"
    },
    {
      "index": 5,
      "csvName": "Type",
      "csharpProperty": "Type",
      "csharpType": "string",
      "nullable": false,
      "description": "Proxy type"
    },
    {
      "index": 6,
      "csvName": "IsDisabled",
      "csharpProperty": "IsDisabled",
      "csharpType": "string",
      "nullable": false,
      "expectedValues": ["True", "False"],
      "description": "Whether proxy is disabled"
    },
    {
      "index": 7,
      "csvName": "Options",
      "csharpProperty": "Options",
      "csharpType": "string",
      "nullable": true,
      "description": "Proxy options"
    },
    {
      "index": 8,
      "csvName": "MaxTasksCount",
      "csharpProperty": "MaxTasksCount",
      "csharpType": "string",
      "nullable": false,
      "description": "Maximum concurrent tasks"
    },
    {
      "index": 9,
      "csvName": "Info",
      "csharpProperty": "Info",
      "csharpType": "string",
      "nullable": true,
      "description": "Additional information"
    }
  ],
  "validationRules": {
    "requiredColumns": ["Id", "Name", "HostId", "Host", "Type", "MaxTasksCount"],
    "guidColumns": ["Id", "HostId"],
    "booleanColumns": ["IsDisabled"],
    "numericColumns": ["MaxTasksCount"]
  }
}
